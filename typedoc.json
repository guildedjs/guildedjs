{
    "name": "guildedjs",
    "mode": "modules",
    "out": "docs",
    "theme": "default",
    "exclude": ["node_modules/**", "dist/**", "types/**"],
    "ignoreCompilerErrors": true,
    "preserveConstEnums": true,
    "excludeExternals": true,
    "external-modulemap": ".*packages\/([^\/]+)\/.*",
    "stripInternal": true,
    "media": "static",
    "readme": "README.md",
    "tsconfig": "tsconfig.base.json"
}