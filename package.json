{
  "name": "@guildedjs/guilded-js",
  "version": "0.0.1",
  "description": "A Node.js API wrapper for the Guilded.gg API written in TS",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "build": "npx lerna run build",
    "bootstrap": "npx lerna bootstrap",
    "lint": "npx eslint packages/ --ext .ts",
    "lint:fix": "npx eslint packages/ --ext .ts --fix",
    "test": "npm run build && node packages/guilded.js/__tests__/index.js",
    "docs": "typedoc --options typedoc.json",
    "postinstall": "npx husky install",
    "prepublishOnly": "npx pinst --disable",
    "postpublish": "npx pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guildedjs/guildedjs.git"
  },
  "keywords": [
    "guilded",
    "guildedjs",
    "guilded.js",
    "guilded-api",
    "wrapper"
  ],
  "author": "Zaid \"Nico\" Arshad <me@zaid-arshad.tech>",
  "license": "MIT",
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 4,
    "endOfLine": "auto"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "discord.js-docgen": "git+https://github.com/zaida04/docgen.git",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^5.0.4",
    "lerna": "^3.22.1",
    "pinst": "^2.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "bugs": {
    "url": "https://github.com/guildedjs/guildedjs/issues"
  },
  "homepage": "https://github.com/guildedjs/guildedjs#readme"
}
